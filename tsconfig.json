{
  "files": [
    "src/export.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "outDir": "ts-temp",
    "lib": ["es2015", "es2015.promise", "es2016", "dom"],
    "typeRoots": [
        "node_modules/@types",
        "typings/globals",
        "typings-other"
    ],
    "types": ["node", "assertion", "atma-utest", "string", "jquery"],
    "declaration": true,
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "experimentalDecorators": true
  }
}