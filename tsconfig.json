{
  "files": [
    "src/export.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "outDir": "ts-temp",
    "typeRoots": [
      "./node_modules/@types",
      "./typings/",
      "./typings-other/"
    ],
    "types": [
      "atma-class",
      "string",
      "node"
    ],
    "declaration": true,
    "target":"es6",
    "module": "commonjs",
    "sourceMap": false,
    "experimentalDecorators": true
  }
}