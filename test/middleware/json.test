
include
    .inject('/lib/io.js')
    .done(function(){
        
        var file = new io.File('test/middleware/data/json.json');
        UTest({
            'read': function(){
                
                eq(typeof file.read().foo, 'string');
            },
            'write': function(){
                var time = Date.now() + '';
                
                file.write({foo: time})
                
                delete file.content
                eq(file.read().foo, time);
            }
        })
    })