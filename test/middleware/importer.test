

include
    .inject('/lib/io.js')
    .done(function(){
        
        io
            .File
            .getHookHandler()
            .register(/./, 'read', 'importer');
        
        var file = new io.File('test/middleware/data/importer.js');
        UTest({
            'read': function(){
                
                var source = file.read();
                
                assert(source.indexOf('bar') > -1);
            }
        })
    })